<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table>
      <tr>
        <th>Email</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Action</th>
      </tr>

      <% for(let i = 0; i< data.length; i++) { %>
      <tr>
        <th><%= data[i].email %></th>
        <th><%= data[i].firstName %></th>
        <th><%= data[i].lastName %></th>
        <th><%= data[i].address %></th>
        <th>
            <a href="/edit-crud/?id=<%= data[i].id %>" class="text-white bg-blue-700">Edit</a>
            <a href="/delete-crud/?id=<%= data[i].id %>" class="text-red-700 bg-slate-50">Delete</a>
        </th>
      </tr>

      <% } %>
    </table>
  </body>
</html>
